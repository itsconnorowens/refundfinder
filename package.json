    "test:all": "npm run test && npm run test:e2e",
    "playwright:install": "playwright install chromium",
    "validate-path": "node scripts/validate-project-path.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@radix-ui/react-accordion": "^1.2.12",

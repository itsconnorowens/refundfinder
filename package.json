    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "happy-dom": "^20.0.8",
    "jsdom": "^27.0.1",
    "msw": "^2.11.6",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^4.0.3"

    "@radix-ui/react-progress": "^1.1.7",
    "@stripe/react-stripe-js": "^5.2.0",
    "@stripe/stripe-js": "^8.1.0",
    "airtable": "^0.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.400.0",
    "next": "15.1.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
